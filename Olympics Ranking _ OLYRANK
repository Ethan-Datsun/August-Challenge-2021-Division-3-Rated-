#include <stdio.h>


int main()
{
    int t;
    scanf("%d", &t);
    while(t--)
    {
        int g1=0, s1=0, p1=0, g2=0, s2=0,p2=0;
        scanf("%d %d %d %d %d %d",&g1,&s1,&p1,&g2,&s2,&p2) ;
        if(g1+s1+p1 > g2+s2+p2)
            printf("1\n");
        else
            printf("2\n");
    }
return 0;
}
